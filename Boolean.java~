public class Boolean {
  
  
  public static void main(String[] args) { 
    
    boolean a=1, b=2, c=3;
    if (a<b) && (a<c)
    {
      System.out.println(a);
    }
    if (b<a) && (b<c)
    {
      System.out.println(c);
    }
    if (c<a) && (c<b)
    {
      System.out.println(c);
    }
  }
}
