class mathematics2
{
  public static void main (String args[])
  {
    System.out.println(2+7/2);
    System.out.println(5*(6+3.0));
    System.out.println(3.0*5/2);
    System.out.println(2*(3/4));
    System.out.println(3*4/5);
    System.out.println(4-(6-(2-7)));
    System.out.println(3/4*2.0);
    System.out.println(3/(4*2.0));
  }
}